{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "paths": {
      "@/assets/*": [
        "assets/*"
      ],
      "@config": [
        "config.ts"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/content/*": [
        "content/*"
      ],
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/data/*": [
        "data/*"
      ]
    },
    "types": [
      "@astrojs/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    ".vscode",
    "dist"
  ]
}